<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cl.medlog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".MenuTabActivity"
            android:label="@string/title_activity_menu_tab"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cl.medlog.MainActivity" />
        </activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".DiagnosticPage" />
        <activity android:name=".FeverPage" />
        <activity android:name=".HeadachePage" />
        <activity android:name=".EarachePage" />
        <activity android:name=".StomachachePage" />
        <activity android:name=".CoughPage" />
        <activity android:name=".SoreThroatPage" />
        <activity android:name=".BlurredVisionPage" />
        <activity android:name=".ParalysisPage" />
        <activity android:name=".NumbnessPage" />
        <activity android:name=".RottingFleshPage" />
        <activity android:name=".DizzinessPage" />
        <activity android:name=".LackOfAHeadPage" />
        <activity android:name=".CancerPage" />
        <activity android:name=".HealthyStuff"></activity>
    </application>

</manifest>