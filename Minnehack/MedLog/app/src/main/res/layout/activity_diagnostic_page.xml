<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/activity_diagnostic_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="12dp"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="2dp"
    tools:context="com.cl.medlog.DiagnosticPage">
    <TextView
        android:text="@string/symptom_checker_welcome"
        android:textSize="6pt"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Fever"
        android:onClick="goToFever"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Headache"
        android:onClick="goToHeadache"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Earache"
        android:onClick="goToEarache"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Stomachache"
        android:onClick="goToStomachache"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cough"
        android:onClick="goToCough"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sore Throat"
        android:onClick="goToSoreThroat"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Blurred Vision"
        android:onClick="goToBlurredVision"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Paralysis"
        android:onClick="goToParalysis"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Numbness"
        android:onClick="goToNumbness"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rotting Flesh"
        android:onClick="goToRottingFlesh"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Dizziness"
        android:onClick="goToDizziness"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lack of a Head"
        android:onClick="goToLackOfAHead"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cancer"
        android:onClick="goToCancer"/>
</LinearLayout>
